<application xmlns="http://xmlns.jcp.org/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="7"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/application_7.xsd">
             
    <description>traffic-ear</description>
    <display-name>traffic-ear</display-name>
    <module>
        <ejb>traffic-ejb.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>traffic-war.war</web-uri>
            <context-root>/traffic</context-root>
        </web>
    </module>
    <module>
        <connector>traffic-rar.rar</connector>
    </module>
    <library-directory>lib</library-directory>
</application>
